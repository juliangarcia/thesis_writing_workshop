	Journal = {Proceedings of the National Academy of Sciences},
	Keywords = {Bayes Theorem,Biological,Disease Progression,Hematopoietic Stem Cells,Hematopoietic Stem Cells: physiology,Humans,Leukemia,Models,Myeloid,Myeloid: etiology,Myeloid: physiopathology,Stem Cell Niche,Stem Cell Niche: physiology,Tumor Microenvironment,Tumor Microenvironment: physiology},
	Number = {10},
	Pages = {3883--3888},
	Title = {{The ecology in the hematopoietic stem cell niche determines the clinical outcome in chronic myeloid leukemia}},
