	Keywords = {BCR-ABL Positive,BCR-ABL Positive:,Benzamides,Cell Proliferation,Chronic,Fusion Proteins,Genetic,Hematopoietic Stem Cells,Hematopoietic Stem Cells: pathology,Humans,Leukemia,Myelogenous,Philadelphia Chromosome,Piperazines,Piperazines: therapeutic use,Protein Kinase Inhibitors,Protein Kinase Inhibitors: therapeutic use,Pyrimidines,Pyrimidines: therapeutic use,Signal Transduction,Translocation,bcr-abl,bcr-abl: metabolism},
