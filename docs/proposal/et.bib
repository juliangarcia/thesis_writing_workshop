@article{ma:JTB:2011,
	Abstract = {Several empirical studies have shown that the animal group size distribution
	of many species can be well fit by power laws with exponential truncation.
	A striking empirical result due to Niwa is that the exponent in these
	power laws is one and the truncation is determined by the average
	group size experienced by an individual. This distribution is known
	as the logarithmic distribution. In this paper we provide first principles
	derivation of the logarithmic distribution and other truncated power
	laws using a site-based merge and split framework. In particular,
	we investigate two such models. Firstly, we look at a model in which
	groups merge whenever they meet but split with a constant probability
	per time step. This generates a distribution similar, but not identical
	to the logarithmic distribution. Secondly, we propose a model, based
	on preferential attachment, that produces the logarithmic distribution
	exactly. Our derivation helps explain why logarithmic distributions
	are so widely observed in nature. The derivation also allows us to
	link splitting and joining behavior to the exponent and truncation
	parameters in power laws.},
	Author = {Ma, Qi and Johansson, Anders and Sumpter, David J.T.},
	Issn = {0022-5193},
	Journal = {Journal of Theoretical Biology},
	Keywords = {Truncated power law, The logarithmic distribution, Merge and split dynamics},
	Number = {1},
	Pages = {35--43},
	Title = {A first principles derivation of animal group size distributions},
	Volume = {283},
	Year = {2011}}

