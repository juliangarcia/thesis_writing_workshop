@article{nemeth:JTB:2010,
	Abstract = {It seems obvious that as the benefits of cooperation increase, the
	share of cooperators in the population should also increase. It is
	well known that positive assortment between cooperative types, for
	instance in spatially structured populations, provide better conditions
	for the evolution of cooperation than complete mixing. This study
	demonstrates that, assuming positive assortment, under most conditions
	higher cooperation benefits also increase the share of cooperators.
	On the other hand, under a specified range of payoff values, when
	at least two payoff parameters are modified, the reverse is true.
	The conditions for this paradox are determined for two-person social
	dilemmas: the Prisoner's Dilemma, the Hawks and Doves game, and the
	Stag Hunt game, assuming global selection and positive assortment.},
	Author = {A. N{\'e}meth AND K. Tak{\'a}cs},
	Date-Added = {2015-06-16 10:15:25 +0000},
	Date-Modified = {2015-06-16 10:19:53 +0000},
	Journal = {Journal of Theoretical Biology},
	Keywords = {Altruism, evolution of cooperation, spatially structured population, price equation, prisoners' dilemma, hawks and doves, Stag hunt},
	Number = {2},
	Pages = {301 - 311},
	Title = {The paradox of cooperation benefits},
	Volume = {264},
	Year = {2010},
