	Journal = {The American Naturalist},
	Note = {PMID: 15540139},
	Number = {S5},
	Pages = {S19-S32},
	Title = {The Evolution of Virulence When Parasites Cause Host Castration and Gigantism.},
	Url = {https://doi.org/10.1086/424606},
	Volume = {164},
	Year = {2004},
