	Number = {8},
	Pages = {2084--91},
	Title = {{Mathematical model of granulocytopoiesis and chronic myelogenous leukemia.}},
	Url = {http://cancerres.aacrjournals.org/cgi/content/abstract/51/8/2084 http://www.ncbi.nlm.nih.gov/pubmed/2009527},
