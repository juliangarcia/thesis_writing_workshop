	Doi = {10.1182/blood-2010-12-326843},
	File = {:Users/boettcher/Documents/Paper/Hamilton et al/Hamilton et al.{\_}2015.pdf:pdf},
	Isbn = {1528-0020 (Electronic)$\backslash$r0006-4971 (Linking)},
	Issn = {0006-4971},
	Journal = {Blood},
	Journaltitle = {Blood},
	Number = {6},
	Pages = {1501--1510},
	Title = {{Chronic myeloid leukemia stem cells are not dependent on Bcr-Abl kinase activity for their survival}},
