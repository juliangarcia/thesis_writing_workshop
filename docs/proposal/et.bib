@article{greenspoon:Evolution:2017,
	Abstract = {The theory that coevolving hosts and parasites create a fluctuating selective environment for one another (i.e., produce Red Queen dynamics) has deep roots in evolutionary biology; yet empirical evidence for Red Queen dynamics remains scarce. Fluctuating coevolutionary dynamics underpin the Red Queen hypothesis for the evolution of sex, as well as hypotheses explaining the persistence of genetic variation under sexual selection, local parasite adaptation, the evolution of mutation rate, and the evolution of nonrandom mating. Coevolutionary models that exhibit Red Queen dynamics typically assume that hosts and parasites encounter one another randomly. However, if related individuals aggregate into family groups or are clustered spatially, related hosts will be more likely to encounter parasites transmitted by genetically similar individuals. Using a model that incorporates familial parasite transmission, we show that a slight degree of familial parasite transmission is sufficient to halt coevolutionary fluctuations. Our results predict that evidence for Red Queen dynamics, and its evolutionary consequences, are most likely to be found in biological systems in which hosts and parasites mix mainly at random, and are less likely to be found in systems with familial aggregation. This presents a challenge to the Red Queen hypothesis and other hypotheses that depend on coevolutionary cycling.},
	Author = {Philip B. Greenspoon and Nicole Mideo},
	Doi = {10.1111/evo.13157},
	Eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/evo.13157},
	Journal = {Evolution},
	Keywords = {Coevolution, fluctuating selection, model, parasites, red queen},
	Number = {3},
	Pages = {747-755},
	Title = {Parasite transmission among relatives halts Red Queen dynamics},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/evo.13157},
	Volume = {71},
	Year = {2017},
	Bdsk-Url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1111/evo.13157},
	Bdsk-Url-2 = {https://dx.doi.org/10.1111/evo.13157}}

@article{hesse:JEB:2015,
	Abstract = {Abstract Abiotic environmental heterogeneity can promote the evolution of diverse resource specialists, which in turn may increase the degree of host--parasite specialization. We coevolved Pseudomonas fluorescens and lytic phage ϕ2 in spatially structured populations, each consisting of two interconnected subpopulations evolving in the same or different nutrient media (homogeneous and heterogeneous environments, respectively). Counter to the normal expectation, host--parasite specialization was significantly lower in heterogeneous compared with homogeneous environments. This result could not be explained by dispersal homogenizing populations, as this would have resulted in the heterogeneous treatments having levels of specialization equal to or greater than that of the homogeneous environments. We argue that selection for costly generalists is greatest when the coevolving species are exposed to diverse environmental conditions and that this can provide an explanation for our results. A simple coevolutionary model of this process suggests that this can be a general mechanism by which environmental heterogeneity can reduce rather than increase host--parasite specialization.},
	Author = {E. Hesse and A. Best and M. Boots and A. R. Hall and A. Buckling},
	Doi = {10.1111/jeb.12689},
	Eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/jeb.12689},
	Journal = {Journal of Evolutionary Biology},
	Keywords = {antagonistic coevolution, bacteria, migration, phages, selection mosaics, specificity},
	Number = {9},
	Pages = {1682-1690},
	Title = {Spatial heterogeneity lowers rather than increases host--parasite specialization},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/jeb.12689},
	Volume = {28},
	Year = {2015},
	Bdsk-Url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1111/jeb.12689},
	Bdsk-Url-2 = {https://dx.doi.org/10.1111/jeb.12689}}

@article{boots:PRSB:1999,
	Abstract = {Why are some diseases more virulent than others? Vector{\textendash}borne diseases such as malaria and water{\textendash}borne diseases such as cholera are generally more virulent than diseases spread by direct contagion. One factor that characterizes both vector{\textendash} and water{\textendash}borne diseases is their ability to spread over long distances, thus causing infection of susceptible individuals distant from the infected individual. Here we show that this ability of the pathogen to infect distant individuals in a spatially structured host population leads to the evolution of a more virulent pathogen. We use a lattice model in which reproduction is local but infection can vary between completely local to completely global. With completely global infection the evolutionarily stable strategy (ESS) is the same as in mean{\textendash}field models while a lower virulence is predicted as infection becomes more local. There is characteristically a period of relatively moderate increase in virulence followed by a more rapid rise with increasing proportions of global infection as we move beyond a {\textquoteleft}critical connectivity{\textquoteright}. In the light of recent work emphasizing the existence of {\textquoteleft}small world{\textquoteright} networks in human populations, our results suggests that if the world is getting {\textquoteleft}smaller{\textquoteright} (as populations become more connected) diseases may evolve higher virulence.},
	Author = {Boots, M. and Sasaki, A.},
	Doi = {10.1098/rspb.1999.0869},
	Eprint = {http://rspb.royalsocietypublishing.org/content/266/1432/1933.full.pdf},
	Issn = {0962-8452},
	Journal = {Proceedings of the Royal Society of London B: Biological Sciences},
	Number = {1432},
	Pages = {1933--1938},
	Publisher = {The Royal Society},
	Title = {{\textquoteleft}Small worlds{\textquoteright} and the evolution of virulence: infection occurs locally and at a distance},
	Url = {http://rspb.royalsocietypublishing.org/content/266/1432/1933},
	Volume = {266},
	Year = {1999},
	Bdsk-Url-1 = {http://rspb.royalsocietypublishing.org/content/266/1432/1933},
