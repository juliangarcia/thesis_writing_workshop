@article{thompson:TEE:1998,
	Abstract = {Rapid evolution of interspecific interactions (during a timespan of
	about 100 years) has the potential to be an important influence on
	the ecological dynamics of communities. However, despite the growing
	number of examples, rapid evolution is still not a standard working
