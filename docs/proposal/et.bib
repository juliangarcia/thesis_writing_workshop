@article{rozhok:PNAS:2016,
	Abstract = {Young children have higher rates of leukemia than young adults. This fact represents a fundamental conundrum, because hematopoietic cells in young children should have fewer mutations (including oncogenic ones) than such cells in adults. Here, we present the results of stochastic modeling of hematopoietic stem cell (HSC) clonal dynamics, which demonstrated that early HSC pools were permissive to clonal evolution driven by drift. We show that drift-driven clonal expansions cooperate with faster HSC cycling in young children to produce conditions that are permissive for accumulation of multiple driver mutations in a single cell. Later in life, clonal evolution was suppressed by stabilizing selection in the larger young adult pools, and it was driven by positive selection at advanced ages in the presence of microenvironmental decline. Overall, our results indicate that leukemogenesis is driven by distinct evolutionary forces in children and adults.},
	Author = {Rozhok, Andrii I and Salstrom, Jennifer L and DeGregori, James},
	Date-Added = {2016-02-12 13:14:05 +0000},
	Date-Modified = {2016-02-12 13:14:26 +0000},
	Doi = {10.1073/pnas.1509333113},
	Journal = {Proceedings of the National Academy of Sciences},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {aging; cancer; childhood leukemia; somatic evolution; stochastic modeling},
	Pages = {1050-1055},
	Pmc = {PMC4743806},
	Pmid = {26755588},
	Pst = {ppublish},
	Title = {Stochastic modeling reveals an evolutionary mechanism underlying elevated rates of childhood leukemia},
