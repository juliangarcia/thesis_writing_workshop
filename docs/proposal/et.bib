	Journal = {Proceedings of the Royal Society B: Biological Sciences},
	Language = {en},
	Month = aug,
	Number = {1373},
	Pages = {1003--1009},
	Title = {Local Adaptation and Gene-For-Gene Coevolution in a Metapopulation Model},
